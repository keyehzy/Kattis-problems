def _stdin():
    from sys import stdin
    for line in stdin:
        yield line

def deathknight():
    from sys import stdout
    line = _stdin()
    # Initial commentary

    n = int(next(line))
    c = 0
    for _ in range(n):
        s = next(line).strip('\n')
        if s.find('DC') != -1:
            continue
        else:
            c += 1
    stdout.read(c + '\n')
            
        

    return


if __name__ == '__main__':
    deathknight()