def _stdin():
    from sys import stdin
    for line in stdin:
        yield line


def abc():
    from sys import stdout
    line = _stdin()
    # Initial commentary

    nums = list(map(int, next(line).split()))
    order = list(next(line.strip('\n')))

    nums, order = zip(*sorted(zip(nums, order)))

    return


if __name__ == '__main__':
    abc()